{
  "api_key": "",
  "endpoint": "https://api.openai.com/v1",
  "api_type": "OPEN_AI",
  "prompts": {
    "default": "Answer any query as briefly, concisely and precisely as possible. Never elaborate on answers unless prompted to do so. For coding questions and answers, simply reply with the code alone and nothing else."
  },
  "conversation": {
    "prompt": "default",
    "context_length": 2,
    "model": "gpt-4o",
    "stream": true,
    "temperature": 0,
    "max_tokens": 1024
  },
  "key_map": {
    "switch_multiline": ["ctrl+j"],
    "submit": ["enter"],
    "multiline_submit": ["ctrl+d"],
    "insert_newline": ["ctrl+d"],
    "multiline_insert_newline": ["enter"],
    "help": ["ctrl+h"],
    "quit": ["ctrl+c"],
    "copy_last_answer": ["ctrl+y"],
    "previous_question": ["ctrl+p"],
    "next_question": ["ctrl+n"],
    "new_conversation": ["ctrl+t"],
    "previous_conversation": ["ctrl+left", "ctrl+g"],
    "next_conversation": ["ctrl+right", "ctrl+o"],
    "remove_conversation": ["ctrl+r"],
    "forget_context": ["ctrl+x"]
  }
}
